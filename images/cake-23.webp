 <script>
 

jQuery('#shippingOptionsModal').on('shown.bs.modal', function (e) {

  var min_time_hours = parseInt('2');

  var dateToday = new Date();
  var hourToday = dateToday.getHours();
  var remainingHours = 24 - min_time_hours;

  jQuery('#delivery_date').datepicker({
        todayHighlight: false, 
        changeYear: false,
        changeMonth: false,
        minDate: hourToday < remainingHours ? 0 : 1,
        dateFormat: "yy-m-dd",
        yearRange: "-100:+20",        
      onSelect: function(dateText, inst) {  
        //$('.shipping-class').slickNext();
        var currentSlide = jQuery('.shipping-class').slick('slickCurrentSlide');
       // alert(currentSlide);
        jQuery('.shipping-class').slick('slickGoTo', currentSlide + 1);

        //alert("You selected "+dateText); //2022-12-19
        /*disabling options if current date selected starts here*/

            var selected_date = new Date(dateText.replace(/-/g, "/")).setHours(0, 0, 0, 0);
            var current_date = new Date().setHours(0, 0, 0, 0);

            if(selected_date == current_date){
                var dt = new Date();

                
                min_time_hours = parseInt('2');

                //var min_time_hours = dt.getHours() + 2;
                min_time_hours = dt.getHours() + min_time_hours;
                var min_time_mins = dt.getMinutes();

                jQuery(".date-listing-wrap").each(function(){ //looping shipping methods
                    let max_slot= jQuery(this).attr('data-max-slot');
                        max_slot = max_slot.split(':');
                        max_slot = ((max_slot[0] * 60) * 60) + (max_slot[1] * 60);
                    let min_time_hours_sec = ((min_time_hours * 60) * 60) + (min_time_mins * 60);
                    
                    if(min_time_hours_sec >= max_slot){
                       jQuery(this).css({"display" : "none"}); 
                    }
                });
                jQuery(".listing-checks").each(function(){ //looping time slots
                    let max_slot= jQuery(this).attr('data-max-time');
                        max_slot = max_slot.split(':');
                        max_slot = ((max_slot[0] * 60) * 60) + (max_slot[1] * 60);
                    let min_time_hours_sec = ((min_time_hours * 60) * 60) + (min_time_mins * 60);
                    
                    if(min_time_hours_sec >= max_slot){
                       jQuery(this).css({"display" : "none"}); 
                    }
                });
                //validation for today past time
                //min_time_hours = 0;
            }else{
              jQuery(".date-listing-wrap").each(function(){ //looping shipping methods
                    jQuery(this).removeAttr("style");
              });
              jQuery(".listing-checks").each(function(){ //looping time slots
                    jQuery(this).removeAttr("style");
              });
            }
        /*disabling options if current date selected ends here*/
      }

  });
  jQuery('.shipping-class').slick('setPosition');
  jQuery('.wrap-modal-slider').addClass('open');
});


jQuery(document).ready(function(){
  //on change validation for options
  jQuery(document).on('change', '#location', function() {
    var product_del_loc = jQuery(this).val();
    if(product_del_loc == '') {
          jQuery("#pa_location").addClass("error");
          jQuery('<span class="val-msgs">Please select location</span>').insertAfter("#pa_location");
    }else{
      jQuery(this).removeClass("error");
      jQuery(this).next('.val-msgs').remove();
    }
  });
    jQuery(document).on('change', '#pa_weight', function() {
    var product_weight = jQuery(this).val();
    if(product_weight == '') {
          jQuery("#pa_weight").addClass("error");
          jQuery('<span class="val-msgs">Please select weight</span>').insertAfter("#pa_weight");
    }else{
      jQuery(this).removeClass("error");
      jQuery(this).next('.val-msgs').remove();
    }
  });
      jQuery(document).on('change', '#pa_flavour', function() {
    var product_flavour = jQuery(this).val();
    if(product_flavour == '') {
          jQuery("#pa_flavour").addClass("error");
          jQuery('<span class="val-msgs">Please select flavour</span>').insertAfter("#pa_flavour");
    }else{
      jQuery(this).removeClass("error");
      jQuery(this).next('.val-msgs').remove();
    }
  });


      jQuery('.single_add_to_cart_button').click(function(e) {
        var product_del_loc = jQuery("#pa_location").val();
        var product_weight = jQuery("#pa_weight").val();
        var product_flavour = jQuery("#pa_flavour").val();
        var product_shipping_date  = jQuery('#product_shipping_date').val();
        var scroll_position = 0;
        var error_found = 0;
        jQuery('.val-msgs').remove();
        jQuery(".custom-select select").removeClass("error");
        jQuery(".delivery-options-wrapper").removeClass("error");
        jQuery("#validation-msgs").html("");

        if(jQuery("#pa_location").length > 0 && product_del_loc == '') {
          jQuery("#pa_location").addClass("error");
          jQuery('<span class="val-msgs">Please select location</span>').insertAfter("#pa_location");
          scroll_position = 200;
          error_found++;
        }
        if(jQuery("#pa_weight").length > 0 && product_weight == ''){
          jQuery("#pa_weight").addClass("error");
          jQuery('<span class="val-msgs">Please select weight</span>').insertAfter("#pa_weight");
          scroll_position = 250;
          error_found++;
        }
        if(jQuery("#pa_flavour").length > 0 && product_flavour == ''){
          jQuery("#pa_flavour").addClass("error");
          jQuery('<span class="val-msgs">Please select flavour</span>').insertAfter("#pa_flavour");
          scroll_position = 300;
          error_found++;
        }
        if(product_shipping_date==''){
          //e.preventDefault();
            jQuery(".delivery-options-wrapper").addClass("error");
            jQuery("#validation-msgs").html('<span class="error">Please select a Delivery Date and Time Options</span>');
            error_found++;

          if( product_del_loc != '' && product_weight != '' && product_flavour != ''){
              jQuery('#shippingOptionsModal').modal('show');
              scroll_position = 250;
            }else{
              scroll_position = 350;
            }
            
           // 
            
        }

        if(error_found > 0){
          jQuery('html, body').animate({
              scrollTop: jQuery("#delivery_date_field").offset().top-scroll_position
          }, 500);
          return false;
        }else{
          return true;
        }
        
      });
    });

jQuery(document).ready(function(){

jQuery('#backarrow').click(function(){
  var current_slick_ind = jQuery('.shipping-class .slick-current').attr('data-slick-index');
  var slick_go_to = 0;
  if(current_slick_ind > 0){ slick_go_to = current_slick_ind - 1; }

  jQuery('.shipping-class').slick('slickGoTo', slick_go_to);
  return false;
});

jQuery('#backtocalendar').click(function (){
  jQuery('.shipping-class').slick('slickGoTo', 0);
  return false;
});

});

